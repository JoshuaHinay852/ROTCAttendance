{
    "name": "rotc/attendance-system",
    "description": "ROTC Attendance System with QR Code",
    "type": "project",
    "require": {
        "endroid/qr-code": "*",
        "endroid/qr-code-bundle": "^4.0",
        "chillerlan/php-qrcode": "^5.0",
        "dompdf/dompdf": "^3.1",
        "league/csv": "9.0.0",
        "mpdf/mpdf": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "minimum-stability": "stable",
        "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "endroid/installer": true
        }
    }
}
